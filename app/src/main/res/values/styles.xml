<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#d8006046</item>
        <item name="colorPrimaryDark">#023C47</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <!--<item name="android:statusBarColor">@android:color/transparent</item>-->
    </style>

    <style name="Widget.MaterialRatingBar.RatingBar" parent="Widget.AppCompat.RatingBar">
        <item name="colorAccent">#6200ea</item>
        <item name="android:colorControlNormal">#f9a825</item>
    </style>

</resources>
